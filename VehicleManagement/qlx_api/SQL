



CREATE TABLE customer(
   customercode       SERIAL PRIMARY KEY,
   lastname            TEXT      NOT NULL,
   fristname           TEXT      NOT NULL,
   age                 INT       NOT NULL,
   address             CHAR(100),
   phone               INT NOT NULL,
   email	             TEXT      NOT NULL,
   gender              bool,
   nationality         TEXT      NOT NULL,
   flag                CHAR(1),         
);





